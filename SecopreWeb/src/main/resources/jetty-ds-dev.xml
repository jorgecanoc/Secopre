<?xml version="1.0"?>
<!DOCTYPE Configure PUBLIC "-//Mort Bay Consulting//DTD Configure//EN" "http://www.eclipse.org/jetty/configure_9_0.dtd">

<Configure id="dataSources" class="org.eclipse.jetty.webapp.WebAppContext">
	<New id="dssecopre" class="org.eclipse.jetty.plus.jndi.Resource">
		<Arg></Arg>
		<Arg>jdbc/dssecopre</Arg>
		<Arg>
			<New class="com.mysql.jdbc.jdbc2.optional.MysqlConnectionPoolDataSource">
				<Set name="Url">jdbc:mysql://localhost:3306/secopre</Set>
				<Set name="User">secopre</Set>
				<Set name="Password">secopre</Set>
			</New>
		</Arg>
	</New>
</Configure>